  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^2.0.2",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "webpack": "4.46",
    "webpack-cli": "4.6.0",
    "webpack-node-externals": "^3.0.0"
